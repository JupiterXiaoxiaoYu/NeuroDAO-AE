{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Header/ConnectWalletButton.js",
     "contexts/WalletProviderContext.js",
    "components/Header/Profile.js",
    "components/Sidebar/NavSection.js",
    "pages/myDAO/index.js",
    "contexts/WalletProviderContext.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
